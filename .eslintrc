{
    "env": {
        "browser": true,
        "es6": true
    },
    "rules": {
        "indent": ["error", 4],
        "no-new": 0
    },
    "overrides": [{
        "files": ["**/*.test.js"],
        "env": {
            "jest": true
        }
    }, {
        "files": ["**/*.base.js"],
        "rules": {
            "class-methods-use-this": 0
        }
    }]
}
